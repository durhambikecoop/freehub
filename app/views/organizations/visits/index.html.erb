<p style="color: green"><%= notice %></p>

<h1>Visits</h1>

<div id="visits">
  <% @visits.each do |visit| %>
    <%= render visit %>
    <p>
      <%= link_to 'Show this visit', organization_visit_path(@org, visit) %>
    </p>
  <% end %>
</div>

<%= link_to 'New visit', new_organization_visit_path(@org) %>
