<div id="<%= dom_id person %>">
  <p>
    <strong>Full name:</strong>
    <%= person.full_name %>
  </p>

  <p>
    <strong>Country:</strong>
    <%= person.country %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= person.address %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= person.email %>
  </p>

  <p>
    <strong>Email opt out:</strong>
    <%= person.email_opt_out %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= person.phone %>
  </p>

  <p>
    <strong>Organization:</strong>
    <%= person.organization_id %>
  </p>

</div>
